{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "API2SF"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/LS_Snowflake1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Its connecting to my Snoflake account",
				"annotations": [],
				"type": "Snowflake",
				"typeProperties": {
					"connectionString": "jdbc:snowflake://ir07211.east-us-2.azure.snowflakecomputing.com/?[object Object]"
				}
			},
			"dependsOn": []
		}
	]
}